{
  "name": "pharit/laravel-logs-viewer",
  "description": "A simple Laravel logs viewer using Bootstrap 5.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "logs", "viewer", "bootstrap"],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Pharit\\LaravelLogsViewer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pharit\\LaravelLogsViewer\\LogsViewerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}


